{
  "name": "formBuilder",
  "version": "1.8.1",
  "main": [
    "dist/*"
  ],
  "homepage": "http://kevinchappell.github.io/formBuilder/",
  "repository": "kevinchappell/formBuilder",
  "authors": [
    "Kevin Chappell <kevin.b.chappell@gmail.com>"
  ],
  "description": "A jQuery plugin for drag and drop form building",
  "keywords": [
    "jquery-plugin",
    "forms",
    "drag and drop",
    "form builder",
    "form create"
  ],
  "license": "MIT",
  "ignore": [
    "**/*",
    "node_modules",
    "test"
  ],
  "config": {
    "files": {
      "test": [
        "test/**/*.spec.js"
      ],
      "formBuilder": {
        "js": [
          "src/js/kc-toggle.js",
          "src/js/form-builder.js",
          "src/js/to-xml.js"
        ],
        "fonts": "src/fonts/fontello",
        "sass": ["src/sass/form-builder.scss"]
      },
      "formRender": {
        "js": [
          "src/js/kc-toggle.js",
          "src/js/form-render.js"
        ],
        "sass": ["src/sass/form-render.scss"]
      },
      "demoSass": [
        "demo/assets/sass/demo.scss"
      ]
    },
    "fontServer": "http://fontello.com"
    },
  "scripts": {
    "prestart": "npm update",
    "test": "gulp test"
  },
  "devDependencies": {
    "babel-core": "^5.8.25",
    "browser-sync": "^2.8.0",
    "codeclimate-test-reporter": "^0.1.0",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^2.3.1",
    "gulp-babel": "^5.2.1",
    "gulp-base64": "^0.1.3",
    "gulp-bump": "^0.3.1",
    "gulp-concat": "^2.6.0",
    "gulp-cssmin": "^0.1.7",
    "gulp-filter": "^3.0.0",
    "gulp-git": "^1.2.4",
    "gulp-header": "^1.2.2",
    "gulp-jshint": "^1.11.2",
    "gulp-load-plugins": "^0.10.0",
    "gulp-plumber": "^1.1.0",
    "gulp-sass": "^2.0.4",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-tag-version": "^1.3.0",
    "gulp-uglify": "^1.2.0",
    "jshint-stylish": "^2.0.1"
  }
}
